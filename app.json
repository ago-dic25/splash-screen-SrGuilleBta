{
  "expo": {
    "name": "Mike Camera",
    "slug": "mike-camera",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logoMike.jpeg",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/logoMike.jpeg",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/logoMike.jpeg",
        "backgroundColor": "#000000"
      }
    },
    "web": {
      "favicon": "./assets/logoMike.jpeg"
    },
    "plugins": [
      "expo-splash-screen"
    ]
  }
}